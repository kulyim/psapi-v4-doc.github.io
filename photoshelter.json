        "type": "object",
        "properties": {
          "about": {
            "type": "string",
            "example": "About"
          },
          "copyright": {
            "type": "string"
          }
        },
        "description": "Standard meta data object"
      },
      "BasicObject": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        },
        "description": "Basic entity data structure"
      },
