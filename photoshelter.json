  "paths": {
    "/gallery": {
      "get": {
        "tags": [
          "Gallery"
        ],
	"lang": "Javascript",
        "summary": "Provides the account galleries",
        "description": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris nec feugiat arcu",
        "responses": {
          "200": {
            "description": "Good job",
            "content": {
