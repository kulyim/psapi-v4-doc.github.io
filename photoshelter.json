        "required": [
          "data"
        ],
        "type": "object",
        "properties": {
          "data": {
            "$ref": "#/components/schemas/Gallery_data"
          }
        }
      },
      "TempErrorModel_error": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          }
        },
        "description": "The HTTP status code applicable to this problem, expressed as a string value",
        "example": "200"
      },
      "ErrorModel_links": {
        "required": [
          "about"
        ],
        "type": "object",
        "properties": {
          "about": {
            "type": "string",
            "description": "A link to the documentation related to this error",
            "example": "https://www.photoshelter.com/developer/index/register/api_key"
          }
        }
      },
      "Gallery_data_attributes": {
        "required": [
          "description"
        ],
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "description": "Gallery description",
            "example": "My trip to DR"
          },
          "f_list": {
            "type": "boolean",
            "description": "Listed on public site",
