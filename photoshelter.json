                "schema": {
                  "$ref": "#/components/schemas/Gallery"
                }
              }
            },
            "links": {
              "GetGalleryById": {
                "operationId": "getGallery",
                "parameters": {
                  "gallery_id": "$response.body#/id"
                },
                "description": "Get a gallery by ID",
                "server": {
                  "url": "https://test.com/"
                }
              }
            }
          },
          "400": {
            "description": "Its a trap"
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            }
          }
        }
      }
    },
    "/gallery/{gallery_id}": {
      "get": {
        "tags": [
          "Gallery"
        ],
        "summary": "Provides a gallery data.",
        "description": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris nec feugiat arcu",
        "operationId": "getGallery",
        "parameters": [
          {
            "name": "gallery_id",
            "in": "path",
            "description": "The gallery Id",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Gallery"
                }
              }
            },
            "links": {
              "GetGalleryById": {
                "operationId": "getGallery",
                "parameters": {
                  "gallery_id": "$response.body#/id"
                },
                "description": "Get a gallery by ID",
                "server": {
                  "url": "https://test.com/"
                }
              }
            }
          },
          "400": {
            "description": "This gallery does not exist"
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TempErrorModel"
                }
              }
            }
          }
        }
      }
    }
  },
